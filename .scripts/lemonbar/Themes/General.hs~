module Themes.General
  ( Icon
  , Property
  ) where

data Icon = { g :: String
            , i :: Int}

data Property = { t :: String
                , v :: String}

instance Show Icon where
  show (Icon g i) = "%{T" ++ i ++ "}" ++ glyph

instance Show Property where
  show (Property t v) = "%{" ++ t ++ v ++ "}"
